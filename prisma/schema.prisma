generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Post {
  id              Int       @id @default(autoincrement())
  projectID       String    @unique
  projectTitle    String
  approvedFunding Int
  approvedMmf     Int
  outcomeMetric   String
  target          String
  ach6Mth         String?
  ach12Mth        String?
  ach18Mth        String?
  ach24Mth        String?
  ach30Mth        String?
  ach36Mth        String?
}
